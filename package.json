    "jquery": "^3.4.1",
    "less": "^4.1.1",
    "less-loader": "^7.0.0",
    "lodash": "^4.17.15",
    "mocha": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loadable": "^5.5.0",
