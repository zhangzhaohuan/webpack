{
  "name": "webpackDemo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.pro.js",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.dev.js --open",
    "smoke": "node ./test/smoke/index.js",
    "unit": "./node_modules/.bin/_mocha",
    "unit_rate": "istanbul cover ./node_modules/.bin/_mocha",
    "lint": "eslint --fix --ext .js,.jsx src",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg"
    }
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "build",
        "chore",
        "docs",
        "feat",
        "fix",
        "perf",
        "refactor",
        "revert",
        "style",
        "test",
        "update"
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.0.0",
    "add-asset-html-webpack-plugin": "^6.0.0",
    "assert": "^2.0.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.1.0",
    "core-js": "^3.34.0",
    "cross-env": "^5.2.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "cssnano": "^6.0.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "glob": "^7.1.6",
    "html-webpack-externals-plugin": "^3.8.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^4.2.5",
    "istanbul": "^0.4.5",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.3",
    "thread-loader": "^4.0.2",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^4.2.2",
    "webpackbar": "^5.0.2"
